    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "minimist": "^1.2.0",
    "neon-cli": "^0.2.0",
    "pre-commit": "^1.2.2",
    "ts-node": "^7.0.1",
    "turbo-gulp": "^0.17.1",
    "typescript": "^3.1.1"
